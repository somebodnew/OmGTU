using System;

namespace ConsoleApplication12
{
    class Program
    {
        
        static void Main(string[] args)
        {
            int N = Convert.ToInt32(Console.ReadLine())-1;
            int[][] a = new int[N][];

            for (int i = 0; i < N; i++)
            {
                a[i] = new int[i+1];
                for (int j = 0; j < a[i].Length; j++) a[i][j] = Convert.ToInt32(Console.ReadLine());
            }



            Console.ReadLine();
        }
    }
}
